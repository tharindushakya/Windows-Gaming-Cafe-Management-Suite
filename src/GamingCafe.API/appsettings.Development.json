{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
  ,
  "RateLimiting": {
    "Prefix": "local",
    "Limit": 20,
    "WindowSeconds": 60,
    "Whitelist": [ "127.0.0.1", "::1" ],
    "ExemptPaths": [ "/health", "/metrics" ],
    "Overrides": {
      "/api/auth": { "Limit": 10, "WindowSeconds": 60 },
      "/api/admin": { "Limit": 5, "WindowSeconds": 60 }
    }
  }
}
